<%= render 'shared/form_error', form: @post %>



<%= form_for @post do |f| %>
<div class="well">
  <%= f.label :title %>
  <%= f.text_field :title %></br>
  <%= f.label :url, 'URL' %>
  <%= f.text_field :url %>
  <%= f.label :description %>
  <%= f.text_area :description,  rows: 7 %>
  </br></br>
  <%= f.submit 'Submit your post' , class: 'btn btn-primary' %>
  </div>
<% end %>

