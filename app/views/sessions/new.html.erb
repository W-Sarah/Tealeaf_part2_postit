<%= render 'shared/content_title', title: "Log in" %>
<div>
<%= form_tag '/login' do  %>
<div class="well">
  <%= label_tag :username %>
  <%= text_field_tag :username %></br>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  </br></br>
  <%= submit_tag 'Login' , class: 'btn btn-primary' %>
  </div>
<% end %>
</div>